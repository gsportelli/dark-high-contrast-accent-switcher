{
  "name": "dark-high-contrast-accent-switcher",
  "displayName": "Dark High Contrast Accent Switcher",
  "description": "Switch accent borders and focus colors for the Default High Contrast Dark theme.",
  "version": "0.0.6",
  "publisher": "gsportelli",
  "engines": {
    "vscode": "^1.101.0"
  },
  "activationEvents": [
    "onCommand:darkHighContrastAccentSwitcher.chooseAccent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "darkHighContrastAccentSwitcher.chooseAccent",
        "title": "Choose Accent (Dark High Contrast)"
      },
      {
        "command": "darkHighContrastAccentSwitcher.resetAccent",
        "title": "Reset Accent (Dark High Contrast)"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gsportelli/dark-high-contrast-accent-switcher.git"
  },
  "bugs": {
    "url": "https://github.com/gsportelli/dark-high-contrast-accent-switcher/issues"
  },
  "homepage": "https://github.com/gsportelli/dark-high-contrast-accent-switcher#readme",
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "accent",
    "borders",
    "high contrast",
    "color",
    "theme",
    "dark",
    "switcher"
  ],
  "icon": "icon.png",
  "dependencies": {},
  "scripts": {},
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
